<%- include('./partials/header.ejs') %> 
<div class="w-full min-h screen  px-10 py-8 text-red ">
  <div class="cards flex flex-wrap gap-10">
    <% user.posts.forEach(function(elem) { %>  
      <div class ="card w-64">
        <div class=" w-full h-40  rounded-lg overflow-hidden">
       
        <img class="object-fit h-48 w-96" src="/images/uploads/<%= elem.image %>" alt="">
        </div>
        <h5 class="mt-2 text-sm"><%= elem.title %></h5>  </div>
    <% }) %>
 
  </div>
</div>

<%- include('./partials/footer.ejs') %>
